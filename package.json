{
  "name": "aws-nodejs-typescript",
  "version": "1.0.0",
  "description": "Serverless webpack example using Typescript",
  "main": "handler.js",
  "scripts": {
    "test": "nyc mocha -r ts-node/register ./**/*.spec.ts",
    "coveralls": "nyc yarn test && nyc report --reporter=text-lcov | coveralls"
  },
  "nyc": {
    "include": [
      "src/**/*.ts",
      "src/**/*.tsx"
    ],
    "extension": [
      ".ts",
      ".tsx"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text-summary",
      "html"
    ],
    "sourceMap": true,
    "instrument": true
  },
  "devDependencies": {
    "@types/aws-lambda": "8.10.11",
    "@types/mocha": "^5.2.5",
    "@types/node": "~8.10.29",
    "@types/request": "^2.47.1",
    "chai": "^4.1.2",
    "coveralls": "^3.0.0",
    "istanbul": "^0.4.5",
    "js-yaml": "^3.10.0",
    "mocha": "^5.1.1",
    "nyc": "^13.0.1",
    "serverless-offline": "^3.25.10",
    "serverless-webpack": "^5.1.3",
    "source-map-support": "^0.5.5",
    "ts-loader": "^5.0.0",
    "ts-node": "^7.0.0",
    "typescript": "^3.0.3",
    "webpack": "^4.17.2"
  },
  "author": "The serverless webpack authors (https://github.com/elastic-coders/serverless-webpack)",
  "license": "MIT",
  "dependencies": {
    "async": "^2.6.0",
    "aws-sdk": "^2.308.0",
    "request": "^2.83.0",
    "request-promise": "^4.2.2",
    "serverless": "^1.30.3",
    "ts-mockito": "^2.3.1",
    "xml-js": "^1.6.7"
  }
}
